{
    "extends": ["eslint-config-egg", "prettier"],
    "rules": {
        "jsdoc/check-tag-names": 0,
        "no-unused-vars": [
            2,
            {
                "vars": "local"
            }
        ],
        "quotes": [
            2,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "no-invalid-this": [0],
        "babel/no-invalid-this": [0],
        "linebreak-style": [0, "error", "windows"],
        "indent": "off",
        "eqeqeq": "off"
        // "prettier/prettier": [2, {
        //   "tabWidth": 4,
        //   "printWidth": 120,
        //   "singleQuote": true,
        //   "trailingComma": "all",
        //   "jsxBracketSameLine": true,
        //   "eslintIntegration": true
        // }]
    },
    "plugins": ["prettier"]
}
